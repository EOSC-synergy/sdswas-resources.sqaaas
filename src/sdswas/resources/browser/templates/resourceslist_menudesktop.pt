<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="sdswas.resources">

<metal:row define-macro="menu" i18n:domain="sdswas.resources">

    <!-- Resources filter menu on desktop -->
    <ul id="resources-filter-menu-desktop" class="filter-menu-desktop">

        <p>Filter by: </p>

        <!-- Btn -->
        <li class="filter-btn-container">
            <button id="publications" class="filter-btn filter-btn-selected btn" type="button"  data-value="publications">Publications</button>
        </li>

        <!-- Btn -->
        <li class="filter-btn-container">
            <button class="filter-btn btn" type="button"  data-value="genericevents">Past Events</button>
        </li>

        <!-- Btn -->
        <li class="filter-btn-container">
            <button class="filter-btn btn" type="button"  data-value="webinars">Webinars</button>
        </li>

        <!-- Btn -->
        <li class="filter-btn-container">
            <button class="filter-btn btn" type="button"  data-value="techreports">Technical reports</button>
        </li>

        <!-- Btn -->
        <li class="filter-btn-container">
            <button class="filter-btn btn" type="button"  data-value="disseminations">Dissemination</button>
        </li>
    </ul><!-- Resources filter menu on desktop ends -->


</metal:row>
 </html>