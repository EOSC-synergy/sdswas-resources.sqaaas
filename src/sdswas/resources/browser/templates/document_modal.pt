<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="sdswas.resources">

<metal:row define-macro="main-content" i18n:domain="sdswas.resources">

  <section class="modwin-text-section">
    <div class="main-wrap modwin-padding-lr">
      <!-- Sidepanel share component -->
      <tal metal:use-macro="context/share_social_networks/macros/sidepanel"/>
      <!-- Sidepanel share component ends-->
    </div>
  </section>

  <section class="modwin-pdf-section">
    <div class="main-wrap main-padding">
      <div class="main-container">
        <div tal:condition="context/file"
          class="col-md-16 embed-responsive embed-responsive-16by9">
          <iframe class="embed-responsive-item"
              tal:attributes="src string:${context/absolute_url}/@@images/file;
                              allowfullscreen True"/>
        </div>
    </div>
  </section>

</metal:row>
</html>