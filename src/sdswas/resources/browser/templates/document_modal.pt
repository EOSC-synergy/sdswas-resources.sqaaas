<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="sdswas.resources">

<metal:row define-macro="main-content" i18n:domain="sdswas.resources">

  <section class="modwin-share-component">
    <div class="modwin-padding-lr">
      <div class="modwin-share-component-container">
        <!-- Sidepanel share component -->
        <tal metal:use-macro="context/share_social_networks/macros/sidepanel"/>
        <!-- Sidepanel share component ends-->
      </div>
    </div>
  </section>

  <!-- Document embed section -->
  <section class="modwin-pdf-section">
    <div class="main-wrap main-padding">
      <metal:macro use-macro="context/document_embed/macros/master"
          tal:define="field_name string:file;
                      file context/file"/>
    </div>
  </section>  <!-- Document embed section ends-->

</metal:row>
</html>