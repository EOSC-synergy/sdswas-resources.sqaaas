<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="sdswas.resources">

<head>
<metal:javascriptslot fill-slot="javascript_head_slot">
  <script type="text/javascript" tal:define="navroot context/@@plone_portal_state/navigation_root_url"
            tal:attributes="src string:${navroot}/++resource++sdswas.resources/js/document.js?1624375391492"/>
</metal:javascriptslot>
</head>

<body>
<metal:main fill-slot="main">

    <!--Page title-->
    <metal:macro use-macro="context/banner_title_macro/macros/master"
            tal:define="title view/view_title"/>

    <div class="main-wrap">

        <!--Resource breadcrum btn -->
        <!--This section does not have a bottom padding because it its followed by a title which do have top padding.
        Thus, this section is different than the homonymous section in dissemination_view-->
        <section tal:define="url string:${portal_url}/resources/">
        <div class="modwin-title-wrap bg-color-white modwin-padding">
            <a class="modwin-back-link" tal:attributes="href url">
            <p class="breadcrum-text"
                tal:content="string:< Back to Resources"></p>
            </a>
        </div><!-- Resource breadcrum btn ends -->
        </section>

        <metal:macro use-macro="context/document_modal/macros/main-content"/>

    </div>

</metal:main>
</body>
</html>