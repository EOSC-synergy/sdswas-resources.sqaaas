<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
 >

<!-- Past events Cards container -->
<div id="genericevents" class="resources-cards-container main-container"
     tal:define="items python:view.generic_events();">
        <tal:row tal:repeat="item items">
                <!-- Card -->
                <div class="resources-card"
                        tal:define="small_info python:item['start'];
                                   url python:item['absolute_url']"
                        tal:attributes="data-gobacklink string:< BACK TO PAST EVENTS;
                                        data-url string:${url}/generic_event_modal;
                                        data-title python:item['title']">
                        <!-- Card content -->
                        <metal:macro use-macro="context/resources_utils/macros/resource_without_preview" />
                </div>
                <!-- Cards container ends -->
        </tal:row>
</div>

</html>
