<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n">

<!-- Webinar Cards container -->
<tal tal:define="b_start python:0;
                b_start request/b_start | b_start;
                batch python: view.webinar_resources( b_size=12, b_start=b_start);">
        <div id="webinars" class="resources-cards-container main-container">
                <tal:row tal:repeat="item batch">
                        <!-- Card -->
                        <div class="resources-card"
                                tal:define="small_info item/start;
                                            url item/absolute_url;
                                            presented_by item/presented_by"
                                tal:attributes="data-breadcumb-text string:< BACK TO WEBINARS;
                                                data-url string:${url}/webinar_modal;
                                                data-title item/title">
                                <metal:macro use-macro="context/resources_utils/macros/resource_without_preview" />
                        </div>
                        <!-- Cards container ends -->
                </tal:row>
        </div>
        <div metal:use-macro="context/batch_macros/macros/navigation" />
</tal>
</html>
