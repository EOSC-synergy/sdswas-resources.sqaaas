<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
 >

    <!-- Publications Cards container -->
    <div id="publications" class="cards-container" tal:define="items python:view.publication_resources()">
            <!-- Card -->
        <div id="" class="resource-card card" tal:repeat="item items"
                tal:define="go_back_link_name string:GO BACK TO PUBLICATIONS;">
            <metal:macro use-macro="context/resourceslist_utils/macros/resource_with_preview" />
            </div><!-- Card ends -->
    </div><!-- Cards container ends -->


    <!-- Past events Cards container -->
    <div id="genericevents" class="cards-container" tal:define="items python:view.generic_events()">
            <!-- Card -->
        <div id="" class="resource-card card" tal:repeat="item items"
                tal:define="go_back_link_name string:GO BACK TO PAST EVENTS;">
            <metal:macro use-macro="context/resourceslist_utils/macros/event_teaser" />
            </div><!-- Card ends -->
    </div><!-- Cards container ends -->

    <!-- Webinar Cards container -->
    <div id="webinars" class="cards-container" tal:define="items python:view.webinar_resources()">
            <!-- Card -->
        <div id="" class="resource-card card" tal:repeat="item items"
                tal:define="go_back_link_name string:GO BACK TO WEBINARS;">
            <metal:macro use-macro="context/resourceslist_utils/macros/event_teaser" />
            </div><!-- Card ends -->
    </div><!-- Cards container ends -->

    <!-- Tech reports Cards container -->
    <div id="techreports" class="cards-container" tal:define="items python:view.techreport_resources()">
            <!-- Card -->
        <div id="" class="resource-card card" tal:repeat="item items"
                tal:define="go_back_link_name string:GO BACK TO TECHNICAL REPORTS;">
            <metal:macro use-macro="context/resourceslist_utils/macros/resource_with_preview" />
            </div><!-- Card ends -->
    </div><!-- Cards container ends -->

    <!-- Disseminations Cards container -->
    <div id="disseminations" class="cards-container" tal:define="items python:view.dissemination_resources()">
            <!-- Card -->
        <div id="" class="resource-card card" tal:repeat="item items"
                tal:define="go_back_link_name string:GO BACK TO DISSEMINATIONS;">
            <metal:macro use-macro="context/resourceslist_utils/macros/resource_without_preview" />
            </div><!-- Card ends -->
    </div><!-- Cards container ends -->

</html>