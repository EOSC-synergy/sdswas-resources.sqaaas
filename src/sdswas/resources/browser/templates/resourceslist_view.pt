<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="sdswas.resources">

<body>

<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main">

      <!--Page title-->
      <section class="banner-top bg-color-white">
          <div class="banner-top-wrap main-wrap main-padding">
              <div class="banner-top-container main-container">
                  <div class="page-title">
                      <h1 tal:content="context/title" />
                  </div>
              </div>
          </div>
      </section>
      <!-- Resources filter menu -->
      <section id="resources-filter-menu-section" class="bg-color-grey-light">
        <div class="resources-filter-menu-wrap main-wrap main-padding">
          <metal:macro use-macro="context/resourceslist_menumobile/macros/menu"/>
          <metal:macro use-macro="context/resourceslist_menudesktop/macros/menu"/>
        </div>
      </section><!-- Resources filter ends -->

      <section class="bg-color-white">

        <!-- <metal:macro use-macro="context/resourceslist_utils/macros/preloader"/>-->
        <div id="resource-cards" class="cards-carpet-wrap main-wrap main-padding">/>

      </section><!-- Resources cards carpet ends -->

      <div id="full_view" class="modal fade" tabindex="-1" role="dialog">
        <metal:macro use-macro="context/resourceslist_modal/macros/modal"/>
     </div>

  </tal:main-macro>
</metal:main>
</body>
</html>